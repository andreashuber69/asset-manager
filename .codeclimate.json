{
  "version": "2",
  "checks": {
    "return-statements": {
      "config": {
        "threshold": 6
      }
    },
    "method-complexity": {
      "config": {
        "threshold": 10
      }
    },
    "similar-code": {
      "config": {
        "threshold": 70
      }
    }
  },
  "exclude_patterns": [
    "**/*.spec.ts"
  ],
  "plugins": {
  }
}
